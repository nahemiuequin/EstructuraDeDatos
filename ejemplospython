numeros=[1,2,3,4,5,6,7]
lista = numeros*5
print (numeros)
print (numeros[3])
# print (numeros[3]) // me muestra la posicion del corchete
for i in numeros:
    print (i)
for i in range(len(numeros)):
    print (numeros[i]) #es el mismo ejercicio de arriba, con otro metodo
numeros.append(3)
print ("Ejercicio Append",numeros) 
numeros.insert(2,5)
print (numeros)
numeros.remove(5) #este caso elimina la primrea vez que se tope con el 5
print (numeros)
print (lista)
numeros.extend(lista)
# numeros.extend(lista) // agrega una lista a la otra
print (numeros)
x=lista.count(5)
print (x)
lista.sort()
print(lista)
